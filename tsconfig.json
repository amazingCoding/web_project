{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "module": "esnext",
    "target": "es5",
    "lib": [
      "es6",
      "dom",
      "es2015.promise"
    ],
    "noImplicitThis": true,
    "jsx": "react",
    "moduleResolution": "node",
  },
  "exclude": [
    "node_modules"
  ]
}